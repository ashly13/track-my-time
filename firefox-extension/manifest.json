{
  "manifest_version": 2,
  "name": "Track My Time",
  "description": "Track the amount of time spent on websites",
  "version": "0.1",
  "icons": {
    "64": "icons/icon.png"
  },

  "permissions": [
    "webRequest",
    "webNavigation",
    "history",
    "<all_urls>"
  ],

  "background": {
    "scripts": [
      "background_script.js"
    ]
  }
}